{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "alias": {
    "@env": ".env"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "semi": ["error", "never"],
    "consistent-return": "off",
    "react/jsx-props-no-spreading": "off",
    "react/forbid-prop-types": "off"
  }
}
